Algoritmo Fecha_nacimiento
	definir edad Como Real
	definir p1,p2,p3 Como Real
	escribir "ingrese la fecha de nacimiento p1(ej;20061127)"
	leer p1
	p1<-(FechaActual()-p1)/10000
	escribir p1
	escribir "ingrese la fecha de nacimiento p2(ej;20061127)"
	leer p2
	p2<-(FechaActual()-p2)/10000
	escribir p2
	escribir"ingrese la fecha de nacimiento p3(eje;20061127)"
	leer p3
	p3<-(FechaActual()-p3)/10000
	escribir p3
	
	
FinAlgoritmo
